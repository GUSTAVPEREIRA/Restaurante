	<queryString language="SQL">
		<![CDATA[SELECT p.cpr_id,p.cpr_abertura, p.cpr_vencimento,p.cpr_tipocondicao, p.cpr_numeroparcela,
p.cpr_pgto, p.cpr_status, p.cpr_valor, p.cpr_valorpgto,r.cp_valortotal, 
cpr_vencimento, contasreceber_cr_id,u.usu_nome
	FROM parcelareceber AS p 
	JOIN contasreceber AS r ON p.contasreceber_cr_id = r.cr_id
	JOIN venda AS v ON v.vend_id = r.venda_vend_id
	JOIN caixa AS c ON c.caixa_id = v.caixa_caixa_id 
	JOIN usuario AS u ON u.usu_id = c.usuario_usu_id
	GROUP BY p.cpr_id,r.cr_id,u.usu_nome]]>
	</queryString>
